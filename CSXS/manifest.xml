<?xml version="1.0" encoding="UTF-8"?>

<ExtensionManifest Version="4.0" ExtensionBundleId="com.example.MPEditor" ExtensionBundleVersion="1.0.0"
		ExtensionBundleName="MPEditor" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ExtensionList>
		<Extension Id="com.example.MPEditor.extension1" Version="1.0" />
	</ExtensionList>
	<ExecutionEnvironment>
		<HostList>
			<Host Name="PHSP" Version="[15.0,15.9]" />
			<Host Name="PHXS" Version="[15.0,15.9]" />
		</HostList>
		<LocaleList>
			<Locale Code="All" />
		</LocaleList>
		<RequiredRuntimeList>
			<RequiredRuntime Name="CSXS" Version="4.0" />

		</RequiredRuntimeList>
	</ExecutionEnvironment>
	<DispatchInfoList>
		<Extension Id="com.example.MPEditor.extension1">
			<DispatchInfo >
				<Resources>
				<MainPath>./index.html</MainPath>
				<!-- <ScriptPath>./jsx/</ScriptPath> -->
				</Resources>
				<Lifecycle>
					<AutoVisible>true</AutoVisible>
				</Lifecycle>
				<UI>
					<Type>Panel</Type>
					<Menu>MPEditor</Menu>
					<Geometry>
						<Size>
							<Height>500</Height>
							<Width>600</Width>
						</Size>
					</Geometry>
					<Icons>
						<Icon Type="Normal"		 >./icons/Logo.png</Icon>
						<Icon Type="RollOver"	 >./icons/Logo.png</Icon>
						<Icon Type="DarkNormal"	 >./icons/Logo.png</Icon>
						<Icon Type="DarkRollOver">./icons/Logo.png</Icon>
					</Icons>
				</UI>
			</DispatchInfo>
		</Extension>
	</DispatchInfoList>
</ExtensionManifest>